{
  "name": "react-vertical-navbar",
  "description": "A simple vertical navbar for react apps",
  "version": "1.1.1",
  "private": false,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@tabler/icons-react": "^2.16.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "del dist && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@storybook/addon-essentials": "7.0.6",
    "@storybook/addon-interactions": "7.0.6",
    "@storybook/addon-links": "7.0.6",
    "@storybook/blocks": "7.0.6",
    "@storybook/preset-create-react-app": "7.0.6",
    "@storybook/react": "7.0.6",
    "@storybook/react-webpack5": "7.0.6",
    "@storybook/testing-library": "0.0.14-next.2",
    "babel-plugin-named-exports-order": "0.0.2",
    "cross-env": "^7.0.3",
    "prop-types": "15.8.1",
    "storybook": "7.0.6",
    "webpack": "5.79.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtoms94/simple-react-navbar"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [ "dist", "README.md" ]
}
